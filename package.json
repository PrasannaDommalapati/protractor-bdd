{
    "name": "Protractor-Bdd",
    "version": "1.0.0",
    "license": "MIT",
    "description": "To demostrate protractor cucumber tests for angular/non angular apps with allure report framework",
    "homepage": "https://github.com/PrasannaDommalapati/protractor-bdd.git",
    "keywords": [
        "protractor",
        "cucumber",
        "typescript",
        "angular",
        "angularjs",
        "testing",
        "behaviour driven development",
        "bdd",
        "selenium",
        "webdriverJS",
        "gherkin",
        "automation testing"
    ],
    "main": "index.js",
    "scripts": {
        "build": "tsc",
        "clean": "rimraf dist/",
        "clean-build": "npm run clean && npm run build",
        "test": "protractor dist/config/protractor.config.js",
        "e2e-wip": "protractor dist/config/protractor.config.js --cucumberOpts.tags=\"@wip\"",
        "webdriver-update": "webdriver-manager update",
        "webdriver-start": "webdriver-manager start"
    },
    "author": "Prasanna Kumar <dpchowdaryd@gmail.com>",
    "devDependencies": {
        "@types/chai": "^4.1.4",
        "@types/chai-as-promised": "^7.1.0",
