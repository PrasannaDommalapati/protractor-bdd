        "@types/fs-extra": "^5.0.4",
        "@types/node": "^10.7.1",
        "@types/selenium-webdriver": "^3.0.10",
        "chai": "^4.1.2",
        "chai-as-promised": "^7.1.1",
        "cucumber": "^4.2.1",
        "cucumber-html-reporter": "^4.0.3",
        "faker": "^4.1.0",
        "firstline": "^2.0.2",
        "fs-extra": "^7.0.0",
        "glob": "^7.1.3",
        "mkdirp": "^0.5.1",
        "protractor": "^5.4.0",
        "protractor-cucumber-framework": "^6.0.0",
        "rimraf": "^2.6.2",
        "ts-node": "^7.0.1",
        "typescript": "^3.0.1",
        "yargs": "^12.0.2"
    },
    "repository": "https://github.com/PrasannaDommalapati/protractor-bdd.git"
}
